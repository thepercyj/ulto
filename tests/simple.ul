# Initialize variables
x = 10
y = 20

# Number of iterations
n = 500
i = 0

print("Initial state:")
print("x:", x)
print("y:", y)

while i < n:
    # Assign new values by adding two-digit numbers
    x = x + 15
    x = x + 20
    y = y + 25
    y = y + 20
    print("Iteration:", i + 1)
    print("Assigned values:")
    print("x:", x)
    print("y:", y)

    # Reverse the state to the previous value
    rev x
    rev y
    print("After reverse:")
    print("x:", x)
    print("y:", y)

    # Increment the iterator
    i = i + 1

print("Completed all iterations.")
