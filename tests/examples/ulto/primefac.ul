# Initialize variables
n = 100
factors = []
i = 2

# Prime factorization
while i * i <= n:
    while n % i == 0:
        factors = factors + [i]
        n = n // i
    i += 1

if n > 1:
    factors = factors + [n]

# Print the factors
print(`Prime factors are:`, factors)

# Reverse the prime factorization
while i != 2:
    rev i
    rev n
    rev factors

# Final print to confirm reversal
print(`After reversal i:`, i)
print(`After reversal n:`, n)
print(`factors:`, factors)